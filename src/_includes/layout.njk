---
title: My default layout title
---
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title }}</title>
  <link rel="stylesheet" href="/style.css">
</head>

<body>
  <nav>
    <ul>
      <li><a href="/">Home</a></li>
      {% for nav in collections.nav %}
        <li><a href="{{ nav.url | url }}">{{ nav.data.title }}</a></li>
      {% endfor %}
    </ul>
  </nav>
  {{ content | safe }}
</body>

</html>
